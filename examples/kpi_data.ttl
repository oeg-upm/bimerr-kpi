data:EN12BimerrKpi rdf:type kpi:EnergyKPI ;
	kpi:identifier "EN12"^^xsd:string ;
	rdfs:label "Cooling energy demand"@en .
	
data:EN13BimerrKpi rdf:type kpi:EnergyKPI ;
